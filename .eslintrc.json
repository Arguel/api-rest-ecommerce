{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "require-jsdoc": "off",
    "quotes": ["error", "double"],
    "max-len": ["warn", {"code": 120}],
    "indent": "off",
    "space-before-function-paren": "off",
    "new-cap": "warn",
    "no-var": "warn",
    "no-invalid-this": "warn",
    "prefer-rest-params": "warn",
    "curly": ["warn", "multi", "consistent"],
    "prefer-spread": "warn",
    "camelcase": "warn"
  }
}
